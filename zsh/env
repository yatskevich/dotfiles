export EDITOR=vim

HOMEBREW=/usr/local/sbin
NPM_PATH=/usr/local/share/npm/bin # npm binaries
export PATH=$HOMEBREW:$NPM_PATH:$PATH

export JAVA_HOME=$(/usr/libexec/java_home -v 1.7)

DEV_ENV=$HOME/dev/env
export PLAY_HOME=$DEV_ENV/play

APPS=$APPS:$JAVA_HOME/bin
APPS=$APPS:$PLAY_HOME
APPS=$APPS:$DEV_ENV/mongodb/bin

export PATH=$PATH:$APPS
